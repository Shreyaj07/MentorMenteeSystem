<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
	id="WebApp_ID" version="3.0">
	<display-name>Mentor_Meeting</display-name>
	<welcome-file-list>
		<welcome-file>login.jsp</welcome-file>
	</welcome-file-list>
	<servlet>
		<servlet-name>student</servlet-name>
		<servlet-class>com.response.Student</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>student</servlet-name>
		<url-pattern>/student</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>mentor</servlet-name>
		<servlet-class>com.response.Mentor</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>mentor</servlet-name>
		<url-pattern>/mentor</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>student issue</servlet-name>
		<servlet-class>com.response.Student_Issue</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>student issue</servlet-name>
		<url-pattern>/studentissue</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>task</servlet-name>
		<servlet-class>com.response.Task</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>task</servlet-name>
		<url-pattern>/task</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>Metting_Attendance</servlet-name>
		<servlet-class>com.response.Metting_Attendance</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Metting_Attendance</servlet-name>
		<url-pattern>/MeetingAttendance</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>Attendance</servlet-name>
		<servlet-class>com.response.Attendance</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Attendance</servlet-name>
		<url-pattern>/attendance</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>Registration</servlet-name>
		<servlet-class>com.response.MentorRegistger</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Registration</servlet-name>
		<url-pattern>/regi</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>Login</servlet-name>
		<servlet-class>com.response.MentorLogin</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Login</servlet-name>
		<url-pattern>/login</url-pattern>
	</servlet-mapping>
</web-app>